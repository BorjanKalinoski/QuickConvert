{
  "name": "QuickConvert",
  "version": "1.0.0",
  "description": "A youtube to mp3 & file conversion app!",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "devStart": "nodemon server.js",
    "backend": "node server.js",
    "frontend:dev": "cd frontend && rm -rf build/ && npm run build:dev",
    "frontend:prod": "cd frontend && rm -rf build/ && npm run build:prod",
    "start:dev": "concurrently \"npm run devStart\" \"npm run frontend:dev\"",
    "start:prod": "concurrently \"npm run frontendProd\" \"npm run backend\""
  },
